<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activities.GameRulesActivity">

    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:text="Правила игры"
              app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="16dp"
              app:layout_constraintHorizontal_bias="0.498" android:id="@+id/textView3">
    </TextView>
    <TextView android:layout_width="367dp" android:layout_height="234dp"
              android:text="@string/Game_rules"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView3"
              android:layout_marginTop="20dp">
    </TextView>
</androidx.constraintlayout.widget.ConstraintLayout>